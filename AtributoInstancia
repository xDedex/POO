package atividade {

public class Carro {
public String modelo;
public String cor;
public int ano;
public String marca;
public double preco;
}

public class Livro {
public String titulo;
public String autor;
public string editora;
public String genero;
public int ano;
}

public class Pessoa {
public String nome;
public double altura;
public int idade;
public String cpf;
public double peso;
}

public class Camisa {
public String tamanho;
public String modelo;
public String cor;
public String marca;
public String referencia;
}

}
